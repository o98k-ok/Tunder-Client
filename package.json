{
  "name": "tunder-http-client",
  "displayName": "Tunder Http Client",
  "description": "Just Fuck Thunder Client",
  "version": "0.1.9",
  "publisher": "o98k-ok",
  "icon": "media/icon.png",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-http-client.start",
    "onCommand:httpClient.loadRequest",
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "httpClient",
          "title": "Tunder Client",
          "icon": "media/icon.png"
        }
      ]
    },
    "views": {
      "httpClient": [
        {
          "id": "httpClientDirectories",
          "name": "HTTP Collections",
          "icon": "$(folder)",
          "contextualTitle": "HTTP Collections"
        }
      ]
    },
    "commands": [
      {
        "command": "vscode-http-client.start",
        "title": "打开[Tunder Client]"
      },
      {
        "command": "httpClient.openStorageFolder",
        "title": "👐 存储目录",
        "icon": "$(folder-opened)"
      },
      {
        "command": "httpClient.createFolder",
        "title": "➕ 创建目录",
        "icon": "$(new-folder)"
      },
      {
        "command": "httpClient.createRequest",
        "title": "➕ 创建请求",
        "icon": "$(new-file)"
      },
      {
        "command": "httpClient.editFolder",
        "title": "✏️ 编辑目录"
      },
      {
        "command": "httpClient.deleteFolder",
        "title": "❌ 删除目录"
      },
      {
        "command": "httpClient.editRequest",
        "title": "✏️ 编辑请求"
      },
      {
        "command": "httpClient.deleteRequest",
        "title": "❌ 删除请求"
      },
      {
        "command": "httpClient.copyRequest",
        "title": "📋 复制请求"
      },
      {
        "command": "httpClient.loadRequest",
        "title": "view more",
        "icon": "$(eye)"
      },
      {
        "command": "httpClient.importCurl",
        "title": "📥 导入 cURL",
        "icon": "$(file-symlink-file)"
      },
      {
        "command": "httpClient.importCurlFromToolbar",
        "title": "📥 导入 cURL 命令",
        "icon": "$(file-symlink-file)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "httpClient.openStorageFolder",
          "when": "view == httpClientDirectories",
          "group": "navigation@1"
        },
        {
          "command": "httpClient.createFolder",
          "when": "view == httpClientDirectories",
          "group": "navigation@2"
        },
        {
          "command": "httpClient.importCurlFromToolbar",
          "when": "view == httpClientDirectories",
          "group": "navigation@3"
        }
      ],
      "view/item/context": [
        {
          "command": "httpClient.createRequest",
          "when": "view == httpClientDirectories && viewItem == directory",
          "group": "inline@1"
        },
        {
          "command": "httpClient.importCurl",
          "when": "view == httpClientDirectories && viewItem == directory",
          "group": "inline@2"
        },
        {
          "command": "httpClient.editFolder",
          "when": "view == httpClientDirectories && viewItem == directory",
          "group": "1_modification"
        },
        {
          "command": "httpClient.deleteFolder",
          "when": "view == httpClientDirectories && viewItem == directory",
          "group": "1_modification"
        },
        {
          "command": "httpClient.loadRequest",
          "when": "view == httpClientDirectories && viewItem == request",
          "group": "inline"
        },
        {
          "command": "httpClient.editRequest",
          "when": "view == httpClientDirectories && viewItem == request",
          "group": "1_modification"
        },
        {
          "command": "httpClient.deleteRequest",
          "when": "view == httpClientDirectories && viewItem == request",
          "group": "1_modification"
        },
        {
          "command": "httpClient.copyRequest",
          "when": "view == httpClientDirectories && viewItem == request",
          "group": "1_modification"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "monaco-editor": "^0.54.0"
  }
}
